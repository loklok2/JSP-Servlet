<%try{
		int dan = Integer.parseInt(request.getParameter("dan")); 
		for (int i =1; i < 10; i++){
			out.print(dan + " * " + i + " = " + (dan * i) + "<br>");
		}
}catch(Exception e){
	out.println("예외 발생: 매개변수 i가 null입니다");
}
%>

