<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
String sel = request.getParameter("sel");

if (sel.equals("type1")) {
    response.sendRedirect("gugudan1.jsp?dan=2");
} else if (sel.equals("type2")) {
    response.sendRedirect("gugudan2.jsp?dir=ver");
} else if (sel.equals("type3")) {
    response.sendRedirect("gugudan3.jsp?col=3");
} else {
    out.println("올바르지 않은 선택입니다.");
}
%>
